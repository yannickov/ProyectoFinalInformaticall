#include <iostream>
using namespace std; 
  
class nueva{
    public:
    string arr[99999];
    int cont=0;
    string* retornary(string a,string b, string c){
        
        arr[cont]=a;cont=cont+1;arr[cont]=b;cont=cont+1;
        arr[cont]=c;cont=cont+1;
        return(arr);
    }
    
};
int main() 
{ 
    nueva A;
    string* arre=A.retornary("hola","que","tal");
    cout<<arre[1];

    return 0; 
} 
